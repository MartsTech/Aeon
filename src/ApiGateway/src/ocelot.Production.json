{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "weather",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/api/weather/{everything}",
      "UpstreamHttpMethod": ["Get"],
      "SwaggerKey": "weather"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "weather",
      "Config": [
        {
          "Name": "Weather API",
          "Version": "v1",
          "Url": "http://weather:5001/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://gateway"
  }
}
