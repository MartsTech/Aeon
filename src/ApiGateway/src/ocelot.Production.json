{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/weather/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "weather",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/api/weather/{everything}",
      "UpstreamHttpMethod": [ "Get" ],
      "SwaggerKey": "weather"
    },
    {
      "DownstreamPathTemplate": "/api/products/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "products",
          "Port": 5002
        }
      ],
      "UpstreamPathTemplate": "/api/products/{everything}",
      "UpstreamHttpMethod": [ "Get", "Put", "Post", "Delete" ],
      "SwaggerKey": "products"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "weather",
      "Config": [
        {
          "Name": "Weather API",
          "Version": "v1",
          "Url": "http://weather:5001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "products",
      "Config": [
        {
          "Name": "Products API",
          "Version": "v1",
          "Url": "http://products:5001/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://gateway"
  }
}
