{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/weather/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "host.docker.internal",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/api/weather/{everything}",
      "UpstreamHttpMethod": ["Get"],
      "SwaggerKey": "weather"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "weather",
      "Config": [
        {
          "Name": "Weather API",
          "Version": "v1",
          "Url": "http://host.docker.internal:5001/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://host.docker.internal"
  }
}
