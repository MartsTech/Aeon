{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/products/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "products",
          "Port": 5001
        }
      ],
      "UpstreamPathTemplate": "/api/products/{everything}",
      "UpstreamHttpMethod": ["Get"],
      "SwaggerKey": "products"
    },
    {
      "DownstreamPathTemplate": "/api/categories/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "categories",
          "Port": 5002
        }
      ],
      "UpstreamPathTemplate": "/api/categories/{everything}",
      "UpstreamHttpMethod": ["Get"],
      "SwaggerKey": "categories"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "products",
      "Config": [
        {
          "Name": "Products API",
          "Version": "v1",
          "Url": "http://products:5001/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "categories",
      "Config": [
        {
          "Name": "Categories API",
          "Version": "v1",
          "Url": "http://categories:5002/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://gateway"
  }
}
