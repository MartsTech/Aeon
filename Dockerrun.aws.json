{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "gateway",
      "image": "martstech/aeon-gateway",
      "hostname": "gateway",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["products", "categories"],
      "memory": 128
    },
    {
      "name": "products",
      "image": "martstech/aeon-products",
      "hostname": "products",
      "essential": false,
      "memory": 128
    },
    {
      "name": "categories",
      "image": "martstech/aeon-categories",
      "hostname": "categories",
      "essential": false,
      "memory": 128
    }
  ]
}
